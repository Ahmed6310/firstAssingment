public class Fibonacci {

	public static void main(String[] args) {
		
		int a = 0;
		int b = 1;
		int sum;
		int i = 0;
		
		sum = a+b;
		
		System.out.print("0, 1, ");
		       
	         while(i<=20){
	        		
	           System.out.print(sum + ", ");
	           a = b;
	           b = sum;
	           sum = a+b;
	           i++;
	         }
	        	        
	    }
	
	}
